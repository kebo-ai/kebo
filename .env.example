# Kebo Mobile App - Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# PostHog Analytics
EXPO_PUBLIC_POSTHOG_API_KEY=your-posthog-api-key

# Backend API
EXPO_PUBLIC_BACKEND_API_URL=https://your-backend-api.com
EXPO_PUBLIC_BACKEND_API_KEY=your-backend-api-key
EXPO_PUBLIC_BACKEND_API_KEY_CATEGORIES=your-categories-api-key
EXPO_PUBLIC_CATEGORIES_API_URL=https://your-categories-api.com

# Google Sign-In (Android)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-google-web-client-id

# Account Deletion (Optional - for production)
EXPO_PUBLIC_SECRET_PASS_KEY_FOR_DELETE_ACCOUNT=your-secret-key
