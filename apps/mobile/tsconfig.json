{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@kebo/shared": [
        "../../packages/shared/src"
      ],
      "@kebo/shared/*": [
        "../../packages/shared/src/*"
      ],
      "@kebo/api": [
        "../../apps/api/src"
      ],
      "@kebo/api/*": [
        "../../apps/api/src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
